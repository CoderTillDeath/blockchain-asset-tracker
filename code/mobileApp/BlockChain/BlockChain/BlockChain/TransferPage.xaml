<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BlockChain.TransferPage">
    <ContentPage.Content>
        <StackLayout>
            <Frame>
                <Label x:Name="propertyId"/>
                </Frame>
            <Frame>
                <Entry x:Name="RecipientID" Placeholder="Enter Recipient ID"/>
                </Frame>
            <Frame>
                <Button Text="Send" Clicked="sendAsset"/>
                </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>