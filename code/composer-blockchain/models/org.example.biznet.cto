/**
 * My commodity trading network
 */
namespace org.acme.biznet
asset Property identified by PropertyId {
    o String PropertyId
    o String description
    --> Trader owner
}
participant Trader identified by traderId {
    o String traderId
    o String firstName
    o String lastName
}
transaction Trade {
    --> Property property
    --> Trader origOwner
    --> Trader newOwner
    --> String longitude
    --> String latitude
}
