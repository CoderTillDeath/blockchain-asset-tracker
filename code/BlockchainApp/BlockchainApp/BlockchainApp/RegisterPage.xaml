<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BlockchainApp.RegisterPage"
             xmlns:local="clr-namespace:BlockchainApp"
             Title="Register">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="UserID" Keyboard="Text" x:Name="userId"/>
            <Entry Placeholder="First Name" Keyboard="Text" x:Name="fName"/>
            <Entry Placeholder="Last Name" Keyboard="Text" x:Name="lName"/>
       

            <Button Text="Register"  Clicked="CompletedRegister"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>